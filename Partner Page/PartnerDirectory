    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const filterButtons = document.querySelectorAll('.filter-btn');
            const partnerCards = document.querySelectorAll('.partner-card');
            const searchInput = document.getElementById('searchInput');
            const partnersGrid = document.querySelector('.partners-grid');
            
            // Add no results message element
            const noResultsMessage = document.createElement('div');
            noResultsMessage.className = 'no-results';
            noResultsMessage.textContent = 'No partners found matching your search criteria.';
            noResultsMessage.style.display = 'none';
            partnersGrid.appendChild(noResultsMessage);
            
            let activeFilter = 'all';
            let searchTerm = '';
            
            // Filter functionality
            filterButtons.forEach(button => {
                button.addEventListener('click', function() {
                    // Remove active class from all buttons
                    filterButtons.forEach(btn => btn.classList.remove('active'));
                    
                    // Add active class to clicked button
                    this.classList.add('active');
                    
                    activeFilter = this.getAttribute('data-filter');
                    filterPartners();
                });
            });
            
            // Search functionality
            searchInput.addEventListener('input', function() {
                searchTerm = this.value.toLowerCase().trim();
                filterPartners();
            });
            
            function filterPartners() {
                let visibleCount = 0;
                
                partnerCards.forEach(card => {
                    const cardCategories = card.getAttribute('data-category').split(',');
                    const partnerName = card.querySelector('h3').textContent.toLowerCase();
                    
                    const categoryMatch = activeFilter === 'all' || cardCategories.includes(activeFilter);
                    const searchMatch = partnerName.includes(searchTerm);
                    
                    if (categoryMatch && searchMatch) {
                        card.style.display = 'flex';
                        visibleCount++;
                    } else {
                        card.style.display = 'none';
                    }
                });
                
                // Show/hide no results message
                if (visibleCount === 0) {
                    noResultsMessage.style.display = 'block';
                } else {
                    noResultsMessage.style.display = 'none';
                }
            }
        });
    </script>
</body>
