            <script>
                // Adjust iframe height to content
                window.addEventListener("load", function() {
                    const body = document.body;
                    const html = document.documentElement;
                    const height = Math.max(
                        body.scrollHeight, body.offsetHeight,
                        html.clientHeight, html.scrollHeight, html.offsetHeight
                    );
                    
                    window.parent.postMessage({
                        type: "setIframeHeight",
                        height: height
                    }, "*");
                });
            </script>
        </body>
        </html>
    '>
    </iframe>

    <script>
        // Listen for height updates from the iframe
        window.addEventListener('message', function(event) {
            if (event.data.type === 'setIframeHeight') {
                document.getElementById('timelineIframe').style.height = event.data.height + 'px';
            }
        });
        
        // Fallback height adjustment
        setTimeout(function() {
            const iframe = document.getElementById('timelineIframe');
            if (iframe.contentWindow && iframe.contentWindow.document.body) {
                iframe.style.height = iframe.contentWindow.document.body.scrollHeight + 'px';
            }
        }, 1000);
    </script>
</body>
</html>
